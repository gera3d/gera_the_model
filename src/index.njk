<style>
  /* Hero Animations - controlled by GSAP now */
  .hero h1, .hero p {
    opacity: 0;
    transform: translateY(40px);
  }

  /* Scroll indicator */
  .scroll-indicator {
    position: absolute;
    bottom: 1.5rem;
    left: 50%;
    transform: translateX(-50%);
    z-index: 10;
    opacity: 0;
  }

  .scroll-indicator span {
    display: block;
    width: 1px;
    height: 60px;
    background: var(--text);
    margin: 0 auto;
    position: relative;
    overflow: hidden;
  }

  .scroll-indicator span::after {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: var(--bg);
    animation: scrollLine 2s ease-in-out infinite;
  }

  @keyframes scrollLine {
    0% { transform: translateY(-100%); }
    50% { transform: translateY(100%); }
    100% { transform: translateY(100%); }
  }

  .scroll-indicator p {
    margin-top: 1rem;
    font-size: 0.6rem;
    letter-spacing: 0.3em;
    text-transform: uppercase;
    color: var(--text-muted);
  }
  
  /* Button hover */
  .contact-btn {
    display: inline-block;
    padding: 1.2rem 4rem;
    border: 1px solid var(--text);
    color: var(--text);
    text-decoration: none;
    font-size: 0.75rem;
    letter-spacing: 0.2em;
    text-transform: uppercase;
    transition: all 0.4s ease;
    position: relative;
    background: transparent;
  }

  .contact-btn:hover {
    background: var(--text);
    color: var(--bg);
  }

  /* Gallery item polish */
  .gallery-item {
    cursor: pointer;
  }

  /* Tone down vivid images */
  .gallery-item.muted img {
    filter: saturate(0.7) brightness(0.95);
  }

  .gallery-item-info span {
    color: #fff;
  }

  /* Social link hover */
  #contact .label:hover {
    color: var(--text) !important;
  }
</style>

<script>
  // Wait for page load then animate hero
  window.addEventListener('load', () => {
    // Hero text animation (after preloader)
    gsap.to('.hero h1', {
      opacity: 1,
      y: 0,
      duration: 1.2,
      delay: 1.3,
      ease: 'power3.out'
    });

    gsap.to('.hero p', {
      opacity: 1,
      y: 0,
      duration: 1,
      delay: 1.6,
      ease: 'power3.out'
    });

    // Scroll indicator
    gsap.to('.scroll-indicator', {
      opacity: 1,
      duration: 1,
      delay: 2,
      ease: 'power2.out'
    });

    // Hide scroll indicator on scroll
    ScrollTrigger.create({
      trigger: '.stats',
      start: 'top 90%',
      onEnter: () => gsap.to('.scroll-indicator', { opacity: 0, duration: 0.5 })
    });

    // Contact section animation
    gsap.from('#contact h2', {
      scrollTrigger: {
        trigger: '#contact',
        start: 'top 75%',
      },
      y: 60,
      opacity: 0,
      duration: 1.2,
      ease: 'power3.out'
    });

    gsap.from('#contact a[href*="mailto"]', {
      scrollTrigger: {
        trigger: '#contact',
        start: 'top 70%',
      },
      y: 30,
      opacity: 0,
      duration: 1,
      delay: 0.2,
      ease: 'power3.out'
    });
  });
</script>
